            "name": "SCTest x64 [apple] (gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/_Build/_Outputs/apple-darwin22.6.0-x86_64-Makefile-clang-15-Debug/SCTest",
